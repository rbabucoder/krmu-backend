{
  "kind": "collectionType",
  "collectionName": "student_achievements",
  "info": {
    "singularName": "student-achievement",
    "pluralName": "student-achievements",
    "displayName": "Student Achievement"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "achivementimage": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "school_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::school-category.school-category",
      "mappedBy": "student_achievements"
    },
    "achievement_content": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    }
  }
}
