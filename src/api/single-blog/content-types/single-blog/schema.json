{
  "kind": "collectionType",
  "collectionName": "single_blogs",
  "info": {
    "singularName": "single-blog",
    "pluralName": "single-blogs",
    "displayName": "Single Blog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "text"
    },
    "blog_slug": {
      "type": "uid",
      "targetField": "title"
    },
    "featured_image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "blog_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog-category.blog-category",
      "mappedBy": "single_blogs"
    },
    "single_blog": {
      "type": "dynamiczone",
      "components": [
        "blog.single-blog-component"
      ]
    }
  }
}
