{
  "kind": "collectionType",
  "collectionName": "school_categories",
  "info": {
    "singularName": "school-category",
    "pluralName": "school-categories",
    "displayName": "School Category"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "description": {
      "type": "text"
    },
    "schools": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::school.school",
      "mappedBy": "school_category"
    },
    "news_and_events": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::news-and-event.news-and-event",
      "inversedBy": "school_categories"
    },
    "faculties": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::faculty.faculty",
      "inversedBy": "school_categories"
    },
    "student_achievements": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::student-achievement.student-achievement",
      "inversedBy": "school_categories"
    }
  }
}
